<img
  class="slide-show__img"
  src="{{#if src}}{{src}}{{else}}#{{/if}}"
  alt="{{#if alt}}{{alt}}{{else}}Slideshow Image{{/if}}"
  title="{{#if title}}{{title}}{{else}}Slideshow Image{{/if}}"
  data-title="{{data.title}}"
  data-duration="{{data.duration}}"
  data-align="{{data.align}}"
  data-bg-color="{{data.bgColor}}"
>